{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix watch\" \"npm run start:dev\"",
    "postinstall": "remix setup node",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js",
    "prepare:husky": "husky install",
    "prepare": "npx prisma db push"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@prisma/client": "^3.9.2",
    "@remix-run/express": "^1.2.1",
    "@remix-run/react": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.28.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.2.1",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-auth-github": "^1.0.0",
    "remix-auth-google": "^1.1.0",
    "remix-utils": "^2.5.0",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.4.1",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "sideEffects": false
}
